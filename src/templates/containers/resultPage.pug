.result-page
    .values
        h1 Исходные данные
        .result-row(v-for="item in values")
            span.result-row__name {{item.name}}
            span.result-row__value {{item.value}}
    .results
        h1 {{msg}}
        .result-row(v-for="item in data")
            span.result-row__name {{item.name}}
            span.result-row__value {{item.value}}

    .diagram-result
        .chart-page
            h2 Средняя теплоемкость воздуха
            .chart-page__air
                scatter-plot(:chart-data="chartAvgTempAir", :name="'.chart-page__air'")

            h2 Средняя теплоемкость дымовых газов
            .chart-page__smoke
                scatter-plot(:chart-data="chartAvgTempSmoke", :name="'.chart-page__smoke'")
